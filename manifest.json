{
  "manifest_version": 3,
  "name": "poof",
  "version": "1.0.0",
  "description": "Hide blocked messages on Discord.",
  "content_scripts": [
    {
      "matches": [
        "https://discord.com/app",
        "https://discord.com/channels/*",
        "https://disocrd.com/guild-discovery",
        "https://discord.com/login",
        "https://discord.com/register",
        "https://discord.com/store"
      ],
      "css": ["poof.css"]
    }
  ]
}
